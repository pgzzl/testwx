/* pages/bottom_menu/bottom_menu.wxss */
.bottom_bar{
    position: absolute;
    width: 100%;
    height: 70px;
    bottom: 0;
    left: 0;
    background: rgb(255, 255, 255);
    display: flex; 
    flex-direction: row;
    justify-content: center;
  }
  .bar_img{
  width: 40px;
  height: 40px;
  }
  .bar_text{
  width: 28px;
  height: 17px;
  color:rgb(170, 170, 170);
  font-family:Inter;
  font-size: 14px;
  font-weight: 500;
  line-height: 25px;
  letter-spacing: 0px;
  }
  .bottom_item{
    height: 70px;
    width: calc(33%);
    display:flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    padding-bottom: 10px;
  }
  

   
  
  

